    <result column="name" property="name" jdbcType="CHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, name
  </sql>
 
  <insert id="myinsert" parameterType="com.wimoor.amazon.adv.sp.pojo.AmzAdvReportPlacement" >
    insert into t_amz_adv_rpt_placement (name)
